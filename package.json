{
  "name": "react-jobly",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-fe\" \"npm run start-be\"",
    "build": "vite build",
    "test": "npx vitest",
    "test:watch": "npx vitest --watch",
    "test:coverage": "npx vitest --coverage",
    "test:ui": "npx vitest --ui",
    "start-fe": "vite --port 3000",
    "start-be": "json-server --watch db.json --delay 500 --port 3001"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "dom": "^0.0.3",
    "dotenv": "^17.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "router": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.4"
  }
}
